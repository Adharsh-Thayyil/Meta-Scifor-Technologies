        "print(\"Hi, I am chatBot.\")\n",
        "name = input(\"What is your name? \")\n",
        "print(\"Oh,\"+name+\" in which grade are you right now?\")\n",
        "grade = int(input())\n",
        "print(name+\" you are in \"+str(grade)+\" grade.Can I ask you one question? Yes or No?\")\n",
        "yes = input(\"\")\n",
        "print(\"Tell me 1024+98=?\")\n",
        "ans = input(\"\")\n",
        "print(\"Good your answer is correct. Bye\")\n",
        "bye = input(\"\")\n",
        "\n"
      ],
